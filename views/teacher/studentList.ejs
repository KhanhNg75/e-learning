<div class="title3" style="padding-right: 45px">
    <p>Student List</p>
    <a href="/teacher/liveStream/<%= courseID %>" class="btn1 btn1--color2">
        <i class="material-icons">
        fast_forward
        </i>
        Start
    </a>
</div>
<div class="c-table">
    <div class="c-table__content">
        <table>
            <thead>
                <tr>
                    <th>Student Name</th>
                    <th>Student Email</th>
                    <th>Join Date</th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                <% for( var i = 0; i < result.length ; i++){%>
                    <tr>
                        <td>
                            <%= result[i].fname%> <%= result[i].lname%>
                        </td>
                        <td>
                            <%= result[i].email%>
                        </td>
                        <td>
                            <%= (result[i].created).toLocaleDateString('en-US') %>
                        </td>
                        <td>
                            <a href="#_"  class="btn1 btn1--color2">
                                <i class="material-icons" style="display:none">
                                    visibility
                                </i>
                                View Profile
                            </a>
                        </td>
                    </tr>
                    <% }; %>
                        <!-- Change to en-GB to date/month/year format-->
            </tbody>
        </table>
    </div>
    <div class="btn1 btn1--color3">
        <a href="/dashboard">Go Back</a>
    </div>
</div>